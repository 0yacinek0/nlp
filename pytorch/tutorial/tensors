      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# [1] Standard numpy-like indexing and slicing\n",
        "tensor = torch.ones(4, 4)\n",
        "print(tensor)\n",
        "print(f\"First row : {tensor[0]}\")\n",
        "print(f\"First column : {tensor[:, 0]}\")\n",
        "print(f\"Last column : {tensor[..., -1]}\")\n",
        "tensor[:, 1] = 0\n",
        "print(tensor)"
      ],
      "metadata": {
